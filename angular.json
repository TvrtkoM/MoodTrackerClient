{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "MoodTrackerClient": {
      "projectType": "application",
      "prefix": "moods",
      "schematics": {
        "@ngrx/schematics:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/MoodTrackerClient",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["src/scss"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "MoodTrackerClient:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "MoodTrackerClient:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "MoodTrackerClient:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "MoodTrackerClient:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "MoodTrackerClient:serve:production"
            }
          }
        }
      }
    },
    "mq-tools": {
      "projectType": "library",
      "root": "projects/mq-tools",
      "sourceRoot": "projects/mq-tools/src",
      "prefix": "mq",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/mq-tools/tsconfig.json",
            "project": "projects/mq-tools/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mq-tools/tsconfig.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mq-tools/src/test.ts",
            "tsConfig": "projects/mq-tools/tsconfig.spec.json",
            "karmaConfig": "projects/mq-tools/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/mq-tools/tsconfig.lib.json",
              "projects/mq-tools/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "datetime-picker": {
      "projectType": "library",
      "root": "projects/datetime-picker",
      "sourceRoot": "projects/datetime-picker/src",
      "prefix": "moods",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/datetime-picker/tsconfig.json",
            "project": "projects/datetime-picker/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/datetime-picker/tsconfig.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/datetime-picker/src/test.ts",
            "tsConfig": "projects/datetime-picker/tsconfig.spec.json",
            "karmaConfig": "projects/datetime-picker/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/datetime-picker/tsconfig.lib.json",
              "projects/datetime-picker/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "MoodTrackerClient",
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}
